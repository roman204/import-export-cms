<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Sanjeev_ImportExportCms::import_export_cms" title="Cms Import Export" sortOrder="100">
                        <resource id="Sanjeev_ImportExportCms::import_export_cms_cmsfile" title="Cms Files"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Sanjeev_ImportExportCms::import_export_cms_config" title="Cms Import Export Section" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
